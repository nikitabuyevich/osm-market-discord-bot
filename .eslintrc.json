{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "fetch": false
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/no-typos": 0,
    "linebreak-style": 0,
    "no-restricted-syntax": 0,
    "max-len": 0,
    "no-console": 0,
    "consistent-return": 0,
    "import/prefer-default-export": "off"
  }
}
